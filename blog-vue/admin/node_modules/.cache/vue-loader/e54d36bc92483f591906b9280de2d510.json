\r\n        code: this.code\r\n      };\r\n      this.axios.post(\"/api/register\", user).then(({ data }) => {\r\n        if (data.flag) {\r\n          this.$message.success(\"注册成功\");\r\n          this.$router.push({ path: \"/login\" });\r\n        } else {\r\n          this.$message.fail(\"注册失敗\");\r\n        }\r\n      });\r\n    },\r\n    \r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0;\r\n  right: 0;\r\n  left: 0;\r\n  background: url(https://static.talkxj.com/config/0w3pdr.jpg) center center /\r\n    cover no-repeat;\r\n}\r\n.login-card {\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0;\r\n  right: 0;\r\n  background: #fff;\r\n  padding: 170px 60px 180px;\r\n  width: 350px;\r\n}\r\n.login-wrap {\r\n  background: #fff;\r\n  width: 400px;\r\n  height: 350px;\r\n  margin: 215px auto;\r\n  overflow: hidden;\r\n  padding-top: 10px 10px, 10px;\r\n  line-height: 40px;\r\n}\r\n.login-title {\r\n  color: #303133;\r\n  font-weight: bold;\r\n  font-size: 1rem;\r\n  text-align:center\r\n}\r\n.login-form {\r\n  margin-top: 1.2rem;\r\n}\r\n.login-card button {\r\n  margin-top: 1rem;\r\n  width: 100%;\r\n}\r\n</style>\r\n"]}]}